{
    "scripts": {
        "updatedeps": "rm -rf ./node_modules && npm i",
        "server:build": "node --harmony ./node_modules/gulp/bin/gulp build",
        "server:build:clean": "node --harmony ./node_modules/gulp/bin/gulp clean build",
        "server:build:watch": "node --harmony ./node_modules/gulp/bin/gulp",
        "server:setup": "node --harmony ./server/setup.js",
        "server:start": "node --harmony ./server/index.js",
        "server:test": "node --harmony ./node_modules/istanbul-harmony/lib/cli.js cover --config ./test/istanbul.yml ./node_modules/mocha/bin/_mocha -- ./test/server",
        "server:test:nocov": "node --harmony ./node_modules/mocha/bin/mocha ./test/server",
        "server:jshint": "node --harmony ./node_modules/jshint/bin/jshint --exclude-path ./.gitignore --verbose --reporter unix .",
        "server:eslint": "node --harmony ./node_modules/eslint/bin/eslint --ignore-path ./.gitignore ./server ./test",
        "server:jscs": "node --harmony ./node_modules/jscs/bin/jscs --esnext --reporter console ./server ./test",
        "server:coveralls": "cat ./test/artifacts/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
        "server:climate": "cat ./test/artifacts/lcov.info | node ./node_modules/codeclimate-test-reporter/bin/codeclimate.js",
        "server:coverage": "npm run server:coveralls && npm run server:climate",
        "server:lint": "npm run server:jshint && npm run server:eslint && npm run server:jscs",
        "test": "npm run server:lint && npm run server:test && npm run server:coverage"
    },
    "dependencies": {
        "bcrypt": "0.8.x",
        "bluebird": "2.9.x",
        "boom": "2.8.x",
        "bunyan": "1.4.x",
        "cookie": "0.1.x",
        "dev-null": "0.1.x",
        "glue": "2.1.x",
        "handlebars": "3.0.x",
        "hapi": "8.8.x",
        "hapi-auth-basic": "3.0.x",
        "hapi-bunyan": "0.6.x",
        "hapi-require-https": "2.0.x",
        "i18n": "0.5.x",
        "joi": "6.6.x",
        "lodash": "3.10.x",
        "moment": "2.10.x",
        "mongodb": "2.0.x",
        "node-statsd": "0.1.x",
        "node-uuid": "1.4.x",
        "nodemailer": "1.4.x",
        "nodemailer-markdown": "1.0.x",
        "statsd-influxdb-backend": "0.6.0",
        "useragent": "2.1.x"
    },
    "devDependencies": {
        "babel": "5.x.x",
        "babel-core": "5.x.x",
        "babel-eslint": "4.x.x",
        "babel-jscs": "2.x.x",
        "chai": "3.2.x",
        "codeclimate-test-reporter": "0.1.x",
        "coveralls": "2.11.x",
        "del": "1.2.x",
        "eslint": "1.0.0-rc-1",
        "gulp": "3.9.x",
        "gulp-babel": "5.x.x",
        "gulp-concat": "2.x.x",
        "gulp-eslint": "1.x.x",
        "gulp-jscs": "2.x.x",
        "gulp-jshint": "1.11.x",
        "gulp-replace": "0.x.x",
        "gulp-sourcemaps": "1.x.x",
        "inject-then": "2.0.x",
        "istanbul-harmony": "0.3.x",
        "jscs": "2.x.x",
        "jshint": "2.x.x",
        "mocha": "2.2.x",
        "promptly": "0.2.x"
    },
    "name": "hapi-getting-started",
    "version": "0.0.4",
    "description": "User management, authentication and authorization api for Node.js using hapi",
    "main": "server/index.js",
    "keywords": [
        "hapijs",
        "getting started"
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com:k-sheth/hapi-getting-started.git"
    },
    "author": {
        "name": "Kumarpal Sheth",
        "email": "k.sheth@gmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/k-sheth/hapi-getting-started/issues"
    },
    "homepage": "https://github.com/k-sheth/hapi-getting-started",
    "engines": {
        "node": ">= 0.12.x"
    },
    "eslintConfig": {
        "ecmaFeatures": {
            "arrowFunctions": true,
            "blockBindings": true,
            "classes": true,
            "defaultParams": true,
            "destructuring": true,
            "forOf": true,
            "generators": true,
            "modules": true,
            "objectLiteralComputedProperties": true,
            "objectLiteralDuplicateProperties": false,
            "objectLiteralShorthandMethods": true,
            "objectLiteralShorthandProperties": true,
            "spread": true,
            "superInFunctions": true,
            "templateStrings": true,
            "jsx": true
        },
        "env": {
            "browser": true,
            "node": true,
            "mocha": true,
            "es6": true
        },
        "globals": {
            "expect": true
        },
        "parser": "babel-eslint",
        "rules": {
            "eol-last": 0,
            "no-alert": 2,
            "no-empty": 2,
            "spaced-line-comment": 0,
            "no-underscore-dangle": 0,
            "no-var": 1,
            "quotes": [
                2,
                "single"
            ],
            "new-cap": 0
        },
        "plugins": []
    },
    "jscsConfig": {
        "esprima": "babel-jscs",
        "esnext": true,
        "requireCurlyBraces": [
            "else",
            "for",
            "while",
            "do",
            "try",
            "catch",
            "case",
            "default"
        ],
        "requireSpaceAfterKeywords": [
            "if",
            "else",
            "for",
            "while",
            "do",
            "switch",
            "return",
            "try",
            "catch"
        ],
        "requireParenthesesAroundIIFE": true,
        "requireSpacesInConditionalExpression": true,
        "requireSpacesInFunctionExpression": {
            "beforeOpeningCurlyBrace": true
        },
        "requireSpacesInFunctionDeclaration": {
            "beforeOpeningCurlyBrace": true
        },
        "disallowMultipleVarDecl": "exceptUndefined",
        "requireBlocksOnNewline": 1,
        "disallowPaddingNewlinesInBlocks": true,
        "disallowEmptyBlocks": true,
        "disallowSpacesInsideParentheses": true,
        "disallowSpaceAfterObjectKeys": true,
        "requireCommaBeforeLineBreak": true,
        "disallowSpacesInCallExpression": true,
        "requireSpaceBeforeObjectValues": true,
        "requireOperatorBeforeLineBreak": [
            "?",
            "+",
            "-",
            "/",
            "*",
            "=",
            "==",
            "===",
            "!=",
            "!==",
            ">",
            ">=",
            "<",
            "<=",
            "||",
            "&&"
        ],
        "disallowSpaceAfterPrefixUnaryOperators": [
            "++",
            "--",
            "+",
            "-",
            "~",
            "!"
        ],
        "disallowSpaceBeforePostfixUnaryOperators": [
            "++",
            "--"
        ],
        "requireSpaceBeforeBinaryOperators": [
            "?",
            "+",
            "-",
            "/",
            "*",
            "=",
            "==",
            "===",
            "!=",
            "!==",
            ">",
            ">=",
            "<",
            "<="
        ],
        "requireSpaceAfterBinaryOperators": [
            "?",
            "+",
            "/",
            "*",
            ":",
            "=",
            "==",
            "===",
            "!=",
            "!==",
            ">",
            ">=",
            "<",
            "<="
        ],
        "disallowSpaceAfterBinaryOperators": [
            "!"
        ],
        "disallowImplicitTypeConversion": [
            "numeric",
            "binary",
            "string"
        ],
        "requireCamelCaseOrUpperCaseIdentifiers": "ignoreProperties",
        "disallowKeywords": [
            "with"
        ],
        "disallowMultipleLineStrings": true,
        "disallowMultipleLineBreaks": true,
        "validateLineBreaks": "LF",
        "validateQuoteMarks": true,
        "validateIndentation": 4,
        "disallowMixedSpacesAndTabs": true,
        "disallowTrailingWhitespace": true,
        "disallowTrailingComma": true,
        "disallowKeywordsOnNewLine": [
            "else",
            "catch"
        ],
        "requireCapitalizedConstructors": {
            "allExcept": [
                "self",
                "toDecorate"
            ]
        },
        "requireDotNotation": {
            "allExcept": [
                "keywords"
            ]
        },
        "validateParameterSeparator": ", ",
        "disallowNewlineBeforeBlockStatements": true,
        "requirePaddingNewlinesBeforeKeywords": [],
        "excludeFiles": [
            "node_modules/**",
            "dist",
            "coverage",
            "public",
            ".idea",
            "test/artifacts"
        ]
    },
    "jshintConfig": {
        "maxerr": 500,
        "bitwise": true,
        "camelcase": true,
        "curly": true,
        "eqeqeq": true,
        "forin": true,
        "immed": false,
        "indent": 4,
        "latedef": true,
        "newcap": true,
        "noarg": true,
        "noempty": false,
        "nonew": false,
        "plusplus": false,
        "quotmark": "single",
        "undef": true,
        "unused": true,
        "strict": false,
        "trailing": false,
        "maxparams": false,
        "maxdepth": false,
        "maxstatements": false,
        "maxcomplexity": 10,
        "maxlen": false,
        "asi": false,
        "boss": false,
        "debug": false,
        "eqnull": false,
        "esnext": true,
        "moz": false,
        "evil": false,
        "expr": true,
        "funcscope": true,
        "globalstrict": true,
        "iterator": false,
        "lastsemic": false,
        "laxbreak": false,
        "laxcomma": false,
        "loopfunc": false,
        "multistr": false,
        "proto": false,
        "scripturl": false,
        "smarttabs": false,
        "shadow": false,
        "sub": false,
        "supernew": false,
        "validthis": false,
        "browser": false,
        "couch": false,
        "devel": true,
        "dojo": false,
        "jquery": false,
        "mootools": false,
        "node": true,
        "nonstandard": true,
        "prototypejs": false,
        "rhino": false,
        "worker": false,
        "wsh": false,
        "yui": false,
        "nomen": false,
        "onevar": false,
        "passfail": false,
        "white": false,
        "predef": [
            "describe",
            "before",
            "beforeEach",
            "after",
            "afterEach",
            "it",
            "expect"
        ]
    },
    "babel": {
        "stage": 0,
        "loose": "all"
    }
}
