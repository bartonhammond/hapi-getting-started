{
    "name": "hapi-getting-started",
    "version": "0.0.4",
    "description": "User management, authentication and authorization api for Node.js using hapi",
    "main": "index.js",
    "scripts": {
        "updatedeps": "rm -rf ./node_modules && npm i",
        "setup": "node --harmony ./server/setup.js",
        "start": "node --harmony ./server/index.js",
        "test": "node --harmony ./node_modules/istanbul-harmony/lib/cli.js cover --config ./test/istanbul.yml ./node_modules/mocha/bin/_mocha -- ./test/server",
        "test:nocov": "node --harmony ./node_modules/mocha/bin/mocha ./test/server",
        "jshint": "node --harmony ./node_modules/jshint/bin/jshint --config ./.jshintrc --verbose --reporter unix .",
        "eslint": "node --harmony ./node_modules/eslint/bin/eslint --config ./.eslintrc --ignore-path ./.eslintignore  ./server ./test ",
        "jscs": "node --harmony ./node_modules/jscs/bin/jscs --config ./.jscsrc --esnext --reporter console ./server ./test",
        "coveralls": "cat ./test/artifacts/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
        "climate": "cat ./test/artifacts/lcov.info | node ./node_modules/codeclimate-test-reporter/bin/codeclimate.js"
    },
    "keywords": [
        "hapijs",
        "getting started"
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com:k-sheth/hapi-getting-started.git"
    },
    "author": {
        "name": "Kumarpal Sheth",
        "email": "k.sheth@gmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/k-sheth/hapi-getting-started/issues"
    },
    "homepage": "https://github.com/k-sheth/hapi-getting-started",
    "engines": {
        "node": ">= 0.12.x"
    },
    "dependencies": {
        "bcrypt": "0.8.x",
        "bluebird": "2.9.x",
        "boom": "2.8.x",
        "bunyan": "1.4.x",
        "cookie": "0.1.x",
        "dev-null": "0.1.x",
        "glue": "2.1.x",
        "handlebars": "3.0.x",
        "hapi": "8.8.x",
        "hapi-auth-basic": "3.0.x",
        "hapi-bunyan": "0.6.x",
        "hapi-require-https": "2.0.x",
        "hoek": "2.14.x",
        "i18n": "0.5.x",
        "joi": "6.5.x",
        "lodash": "3.10.x",
        "lout": "6.2.x",
        "moment": "2.10.x",
        "mongodb": "2.0.x",
        "node-statsd": "0.1.x",
        "node-uuid": "1.4.x",
        "nodemailer": "1.4.x",
        "nodemailer-markdown": "1.0.x",
        "statsd-influxdb-backend": "0.6.0",
        "tv": "4.1.x",
        "useragent": "2.1.x"
    },
    "devDependencies": {
        "babel": "5.6.x",
        "babel-core": "5.7.x",
        "babel-eslint": "3.1.x",
        "babel-jscs": "2.0.x",
        "chai": "3.1.x",
        "codeclimate-test-reporter": "0.1.x",
        "coveralls": "2.11.x",
        "eslint": "1.0.0-rc-1",
        "eslint-plugin-react": "2.7.x",
        "esprima-fb": "15001.1.0-dev-harmony-fb",
        "inject-then": "2.0.x",
        "istanbul-harmony": "0.3.x",
        "jscs": "1.13.x",
        "jshint": "2.8.x",
        "mocha": "2.2.x",
        "nodemon": "1.3.x",
        "promptly": "0.2.x"
    }
}
