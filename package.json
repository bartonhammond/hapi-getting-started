{
    "name": "hapi-getting-started",
    "version": "0.0.4",
    "description": "User management, authentication and authorization api for Node.js using hapi",
    "main": "index.js",
    "scripts": {
        "setup": "node --harmony ./setup.js",
        "start": "node --harmony ./index.js",
        "test": "node --harmony ./node_modules/lab/bin/lab --verbose --reporter console",
        "test-with-report": "node --harmony ./node_modules/lab/bin/lab --coverage --reporter html --output ./test/artifacts/coverage.html",
        "test-coverage": "node --harmony ./node_modules/lab/bin/lab --coverage --reporter lcov --output ./test/artifacts/lcov.info",
        "coveralls": "cat ./test/artifacts/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
        "climate": "cat ./test/artifacts/lcov.info | node ./node_modules/codeclimate-test-reporter/bin/codeclimate.js",
        "jshint": "node ./node_modules/jshint/bin/jshint --config .jshintrc --verbose --reporter unix *.js ./server/**/*.js ./test/**/*.js",
        "statsd": "node ./statsd/stats.js ./statsd.config"
    },
    "keywords": [
        "hapijs",
        "getting started"
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com:k-sheth/hapi-getting-started.git"
    },
    "author": {
        "name": "Kumarpal Sheth",
        "email": "k.sheth@gmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/k-sheth/hapi-getting-started/issues"
    },
    "homepage": "https://github.com/k-sheth/hapi-getting-started",
    "engines": {
        "node": ">= 0.10.x"
    },
    "dependencies": {
        "bcrypt": "0.8.x",
        "bluebird": "2.9.x",
        "boom": "2.x.x",
        "bunyan": "1.3.x",
        "dev-null": "0.1.x",
        "glue": "2.0.x",
        "handlebars": "3.0.x",
        "hapi": "8.x.x",
        "hapi-auth-basic": "2.x.x",
        "hapi-bunyan": "0.6.x",
        "hapi-require-https": "2.0.x",
        "hoek": "2.x.x",
        "i18n": "0.5.x",
        "joi": "6.x.x",
        "lodash": "3.7.x",
        "lout": "6.x.x",
        "mkdirp": "0.5.x",
        "moment": "2.10.x",
        "mongodb": "2.0.x",
        "node-statsd": "0.1.x",
        "node-uuid": "1.4.x",
        "nodemailer": "1.3.x",
        "nodemailer-markdown": "1.0.x",
        "poop": "1.x.x",
        "tv": "4.x.x",
        "useragent": "2.1.x"
    },
    "devDependencies": {
        "code": "1.x.x",
        "codeclimate-test-reporter": "0.0.x",
        "coveralls": "2.11.x",
        "jshint": "2.7.x",
        "lab": "5.x.x",
        "promptly": "0.2.x",
        "rimraf": "2.3.x"
    }
}
