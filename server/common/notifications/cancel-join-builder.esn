'use strict';
import * as utils from './../utils';
export default function(toAdd) {
    return function(obj, request, notification) {
        let modified = false;
        request.payload[toAdd].forEach((a) => {
            if (notification.content.join === a) {
                modified = true;
                notification.setState('cancelled', utils.by(request));
            }
        });
        return modified ? notification.save() : notification;
    };
}
